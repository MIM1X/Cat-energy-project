{
  "name": "cat-energy",
  "private": true,
  "description": "Личный проект от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^3.0.0",
    "autoprefixer": "10.4.0",
    "browser-sync": "^3.0.3",
    "del": "^8.0.0",
    "gulp": "^5.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^9.1.0",
    "gulp-less": "5.0.0",
    "gulp-libsquoosh": "^1.1.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-svgstore": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-webp": "^5.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "postcss": "8.3.11",
    "stylelint": "^16.14.1",
    "stylelint-config-htmlacademy": "^4.2.3"
  },
  "overrides": {
    "es5-ext": "0.10.53"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "lint": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp dev"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "engines": {
    "node": "^16"
  },
  "type": "module",
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
